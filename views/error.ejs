<% layout('layouts/boilerplate') %>
<div class="row">
  <div class="col-10 offset-1">
    <div class="alert alert-danger" role="alert">
      <h1 class="alert-heading h4"><%= err.message %></h1>
      <% if (process.env.NODE_ENV !== 'production') { %>
      <p><%= err.stack %></p>
      <% } %>
    </div>
  </div>
</div>
